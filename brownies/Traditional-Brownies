							var user_comment_width = $('.ratePrintAddDiv').width();
							user_comment_width *= 1.10;
							if(user_comment_width < 400){
								user_comment_width = 400;
							}
									
							var feedback_div_obj = $(".addToMyBoxFeedbackDiv");
							$(feedback_div_obj).css(
								{
									'width': user_comment_width,
									'z-index': 9999
								}
							);
							
							var total_width = $('#mainLayoutDiv2').width();
							var div_width = $(feedback_div_obj).width();
							var div_heigth = $(feedback_div_obj).height();
							var top = ($(window).scrollTop());
							
							if(isNaN(total_width)  ||  total_width == null){

																		var article_div_obj = $('#col2 .articleDiv');
										
								total_width = $(article_div_obj).width();
								var article_div_position = $(article_div_obj).position();
								top -= article_div_position.top;
							}
							
							top += 20;
							var left = ( total_width - div_width ) / 2;
									
							if(isNaN(top)){
								top = 0;
							}
							
							if(isNaN(left)){
								left = 0;
							}

							$(".addToMyBoxFeedbackDiv").css(
								{
									'top': top + "px",
									'left': left + "px"
								}
							);
							
