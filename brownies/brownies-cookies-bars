  #hptoprolloverbtn { background:url('http://www.midwestliving.com/sites/midwestliving.com/themes/mwl_theme/images/icons/promo_more.png') no-repeat; }
  #hptoprolloverclose { background:url('http://www.midwestliving.com/sites/midwestliving.com/themes/mwl_theme/images/icons/promo_close.png') no-repeat; }
</style>
<script>
  $(document).ready(function() {
    var config = { interval: 200, over: boxOpen, timeout: 200, out: boxClose };
    function boxOpen() { $('#hptoprolloverlayer').slideDown(100).css('display', 'block'); }
    function boxClose() { /* Do Nothing */ }
    $('#hptoprollover').hoverIntent(config);
    $('html').click(function() { $('#hptoprolloverlayer').slideUp(100); });
    $('#hptoprolloverclose').click(function() { $('#hptoprolloverlayer').slideUp(100); });
    /* hitting the esc key closes the window */
    $(document).keyup(function(e) { if (e.keyCode == 27) { $('#hptoprolloverclose').click(); } });
  });
</script>
<div id="hptoprollover">
  <div class="headerTeaser parbase teaser">
    <div class="headerrolldown section" id="div-mone-tbr-teaser">
      <a class="clearfix" href="https://secure.diabeticlivingonline.com/order/?containerName=i47gutb40" target="_blank"><img height="70" src="http://images.diabeticlivingonline.mdpcdn.com/sites/all/modules/custom/monetate/temporary/dlv_rolldown_top_Pay0_320x70_9655.jpg" width="320"></a>
      <div id="hptoprolloverbtn">More</div>
      <div id="hptoprolloverlayer">
        <div id="div-mone-tbr-over">
          <a class="clearfix" href="https://secure.diabeticlivingonline.com/order/?containerName=i47gutb40" target="_blank"><img height="550" src="http://images.diabeticlivingonline.mdpcdn.com/sites/all/modules/custom/monetate/temporary/dlv_rolldown_banner_Pay0_440x550_9655.jpg" width="440"></a>
        </div>
        <div id="hptoprolloverclose">Close</div>
