        <script type="text/javascript">

            $(function(){
                // assign the slider to a variable
                var slider = $('#kidsRecipeSlide').bxSlider({
                    controls: false,
                                            auto: true,
                                            pause: 6000,
                    onBeforeSlide: function(currentSlide, totalSlides){
                        // remove all active classes
                        $('.thumbs a').removeClass('pager-active');
                        // assisgn "pager-active" to clicked thumb
                        $('.thumbs a').eq(currentSlide).addClass('pager-active');
                        //f_path, f_title, f_intro for arrays
                        //fTitle (innerHtml), fIntro (innerHtml) for things to update
                        var a_paths = new Array();
                        var a_titles = new Array();
                        var a_intros = new Array();
                        var thePaths = document.getElementById('f_path');
                        var theTitles = document.getElementById('f_title');
                        var theIntros = document.getElementById('f_intro');
                        a_paths = thePaths.value.split('||');
                        a_titles = theTitles.value.split('||');
                        a_intros = theIntros.value.split('||');
                        var fTitle = document.getElementById('featuredTitle');
                        var fIntro = document.getElementById('featuredIntro');
                        fTitle.innerHTML = '<a href="'+a_paths[currentSlide]+'">'+a_titles[currentSlide]+'</a>';
                        fIntro.innerHTML = a_intros[currentSlide];
                    }
                });

                // assign a click event to the external thumbnails
                $('.thumbs a').click(function(){
                    var thumbIndex = $('.thumbs a').index(this);
                    // call the "goToSlide" public function
                    slider.goToSlide(thumbIndex);

                    // remove all active classes
                    $('.thumbs a').removeClass('pager-active');
                    // assisgn "pager-active" to clicked thumb
                    $(this).addClass('pager-active');
                    // very important! you must kill the links default behavior
                    return false;
                });

                $('#go-prev1').click(function(){
                    slider.goToPreviousSlide();
                    return false;
                });

                $('#go-next1').click(function(){
                    slider.goToNextSlide();
                    return false;
                });
                // assign "pager-active" class to the first thumb
                $('.thumbs a:first').addClass('pager-active');

            });

            function MM_changeProp(objId,x,theProp,theValue) { //v9.0
                var obj = null; with (document){ if (getElementById)
