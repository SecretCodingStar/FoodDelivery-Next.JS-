    					<!-- START private note js -->
		
		<script type="text/javascript">
			
			var private_note = {};
			private_note.sid = 28;
			private_note.text = false;
			private_note.use_bg_img = true;
			private_note.tpl_id = '2100';
			private_note.display_link_obj;
			/**
			 * this is actually not true when the page loads. the form will not be displayed
			 * however, the workflow requires this to be set to true on page ready
			 * */
			private_note.form_displayed = true;
			
			
			private_note.aID = '69451';
			private_note.open_private_note_key = 'autoOpenPrivateNote_69451';
			
			
			switch(private_note.sid){
				case 28: // mr food
				case 53: // everyday diabetic recipes
					private_note.use_bg_img = false;
					break;
			}
			
							private_note.user_logged_in = false;
							
			$(document).ready(
				function(){
					
					switch(private_note.tpl_id){
						case '2100':
						case '2105':
							private_note.display_link_obj = $('.actionLinks a.notes');
							break;
						default:
							private_note.display_link_obj = $('.articleDiv .privateNoteDiv a.privateNoteLink');
							break;
					}
					
					$(private_note.display_link_obj).click(
						function(){
							
							console.log('private_note.display_link_obj click');
							
							if(private_note.user_logged_in){
								toggleNoteContent();
								
							}else{
								login_dhtml_obj.displayLoginDhtmlBox('dhtmlLogin', 'You must be logged in to add a private note.');
								
								private_note.setOpenNoteOnReload();
							}
							
							
							return false;
						}
					);
					
					
					
					
					
					//var note_btn = $('.ratePrintAddTable .notesTd a, .ratePrintAddDiv .notes a');
					var note_btn = $('.ratePrintAddDiv .notes a');
					
					/*
    					2017-09-20
					if(private_note.use_bg_img == true){
						$(note_btn).html('').css(
							{
								'background-image' : 'url(//static.primecp.com/images/buttons-article-page.png)',
								'width' : '45px',
								'height' : '22px',
								'background-position' : '0 -210px'
							}
						).hover(
							function(){
								$(this).css('background-position', '-200px -210px');
							},
							function(){
								$(this).css('background-position', '0 -210px');
							}
						);
					}*/
					
					$(note_btn).click(
						/**
						 *private note button
						 */
						function(){
							console.log('notes button clicked');
							
							if(private_note.user_logged_in){
								/**
								*2011-8-4
								*scroll to the notes content
								*/
								
								var top_obj = $("#col2");
								if(top_obj.length < 1){
    								top_obj = $(".privateNoteDiv");
                                }
                                if(top_obj.length > 0){
                                    $('html,body').animate({scrollTop: $(top_obj).offset().top},'slow');
                                }
								
								toggleNoteContent();
							}else{
								login_dhtml_obj.displayLoginDhtmlBox('dhtmlLogin', 'You must be logged in to add a private note.');
								
								private_note.setOpenNoteOnReload();
							}
							
							return false;
						}
					);
					
					/**
					 *add note form cancel button
					*/
					$('.articleDiv .privateNoteDiv input.cancel').click(
						function(){
							/**
							 *we are using toggle, but this will always hide the content
							 *how else would the cancel button be displaying?
							*/
							toggleNoteContent();
							
							return false;
						}
					);
					
					/**
					 *add note form
					*/
					$('#privateNoteForm').ajaxForm(
						{
							beforeSubmit: ajaxFormPreSubmit_privateNoteForm,
							dataType: 'html',
							complete: function(){
								toggleNoteContent();
								
								$('#privateNoteForm').fadeTo("slow", 1.0);
							},
							success: function(html) {
								initPrivateNote(html);
							}
						}
					);
					
					// 2016-02-16
					if(typeof(Storage) !== "undefined") {
    					var open_key = localStorage.getItem(private_note.open_private_note_key);
    					if(open_key != undefined  &&  open_key == private_note.aID){
        				    // only try to open the botes if logged in (gets too loopy otherwise)
        				    if(private_note.user_logged_in){
            				    
            				    private_note.form_displayed = false; // needed to get the toggle right
                            }
                            // unset the key
        				    localStorage.setItem(private_note.open_private_note_key, 0);
        				}
    				}
    				getPrivateNoteText();
					
				} // end main func
			); // end ready;
			
			
			function initPrivateNote(html){
				/**
				*add the note text to the html version
			   */
			   $(".privateNoteText span").html(html);
			   
			   /**
				*add the note text to the text area
			   */
			   $("#mynotes").val(html);
					
				if(html != ''){
					private_note.text = html;
				}else{
					private_note.text = false;
				}
			}
			
			function toggleNoteContent(){
				if(private_note.form_displayed == true){
					
					$('#privateNoteForm').slideUp('slow');
					private_note.form_displayed = false;
					
					if(private_note.text != false){
						/**
						 *if we have text, show it
						 **/
						$('.privateNoteText').slideDown('slow');
					}
				}else{
					
					$('#privateNoteForm').slideDown('slow');
					private_note.form_displayed = true;
					/**
					 *if we are showing the form we are always hiding the text
					 **/
					$('.privateNoteText').slideUp('slow');
				}
				
				
			}
							
							
			
			function getPrivateNoteText(){
				//alert('getPrivateNoteText');
				
				$.ajax({
					url: "/ajax_process.php",
					type: 'GET',
					cache: false,
					data: (
						{
							aID: '69451',
							task: 'private_note_get'
						}
					),
					dataType: "html",
					timeout: 10000,
					complete: function(){
					},
					success: function(html){
						initPrivateNote(html);
						toggleNoteContent();
					}
				});
			}
			
			function ajaxFormPreSubmit_privateNoteForm() {
                $('#privateNoteForm').fadeTo("slow", .3);
				return true;
			}
			
			
			// 2016-02-16
			private_note.setOpenNoteOnReload = function(){
    			console.log('private_note.setOpenNoteOnReload: ' + private_note.open_private_note_key);
    			// confirm the browser supports localStorage
    			if(typeof(Storage) !== "undefined") {
                    localStorage.setItem(private_note.open_private_note_key, private_note.aID);
                }
            }
			
		</script>
