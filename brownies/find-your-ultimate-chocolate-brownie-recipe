<script type='text/javascript'>
    // get some more meta into HRST.targeting
    var metaTags=document.getElementsByTagName("meta");

    var ogAuthorContent = "";
    var ogTitleContent = "";

    for (var i = 0; i < metaTags.length; i++) {
        if (metaTags[i].getAttribute("property") === "og:author") {
            ogAuthorContent = metaTags[i].getAttribute("content");
        }
        if (metaTags[i].getAttribute("property") === "og:title") {
            ogTitleContent = metaTags[i].getAttribute("content");
        }
    }
    if(HRST.targeting.author === '' ) {
        HRST.targeting.author = ogAuthorContent;
    }
    if(HRST.targeting.page === '' ) {
        HRST.targeting.page = ogTitleContent;
    }

</script>
