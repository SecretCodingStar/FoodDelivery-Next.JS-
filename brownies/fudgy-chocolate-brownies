              <!-- Ingredients -->
              <div class="partial recipe-ingredients">
                <h2>Ingredients</h2>
              
                  <div class="ingredients">
                    <ul>
                        <li>  nonstick cooking spray</li>
                        <li>3/4 cup all-purpose flour, spooned and leveled</li>
                        <li>1/3 cup unsweetened cocoa powder</li>
                        <li>1/4 teaspoon kosher salt</li>
                        <li>1/2 cup (1 stick) unsalted butter, cut into pieces</li>
                        <li>6 ounces semisweet or bittersweet chocolate, chopped</li>
                        <li>1 1/2 cups sugar</li>
                        <li>1 teaspoon pure vanilla extract</li>
                        <li>4  large eggs</li>
                    </ul>
                  </div>
              </div>
          
              <!-- Chicory -->
                <div class="chicory-order-ingredients"></div>
                <script async defer type="text/javascript" src="//www.chicoryapp.com/widget_v2/"></script>
          
              <!-- Nutrition -->
