		<script type="text/javascript">			
			var article_nutrition_obj = {};
			article_nutrition_obj.nutrition_label_obj;
			article_nutrition_obj.toggle_link_obj;
			
			article_nutrition_obj.aID = '69451';
			
			$(document).ready(
				function() {
					article_nutrition_obj.nutrition_label_obj = $('#articlePageNutritionLabel');
					article_nutrition_obj.toggle_link_obj = $(article_nutrition_obj.nutrition_label_obj).find('h2 a');
					
					$(article_nutrition_obj.toggle_link_obj).click(
						function(){
    						
    						$(this).html( ($(this).html() == 'Show More') ? 'Show Less' : 'Show More' );
    						
    						if($(article_nutrition_obj.nutrition_label_obj).hasClass('noPrint')){
                                $(article_nutrition_obj.nutrition_label_obj).removeClass('noPrint')
                            }else{
                                $(article_nutrition_obj.nutrition_label_obj).addClass('noPrint')
                            }
    						
    						$(article_nutrition_obj.nutrition_label_obj).children(':not(h2)').slideToggle();
    						    						
    						return false;
						}
					);
					
					$('.detailedNutritionLink').click(
						function(){
    						console.log('click - detailedNutritionLink');
    						
							initDetailedNutritionDhtml();
							return false;
						}
					);
				}
			);
			
			function initDetailedNutritionDhtml(){
				
				$('#detailedNutritionDhtml').remove();
				
				$.ajax({
					url: "/ajax_process.php",
					type: 'GET',
					cache: false,
					dataType: "html",
					timeout: 10000,
					data: (
						{
							task: 'get_detailed_nutrition_html',
							aID: article_nutrition_obj.aID
						}
					),
					complete: function(){
					},
					success: function(html){
						$('body').append('<div id="detailedNutritionWrapper"><div id="detailedNutritionDhtml">' + html + '</div></div>');
						var nut_label_offset = $('#articlePageNutritionLabel').offset();
						
						$('#detailedNutritionWrapper').css(
							{
								'top': '0px',
								'left': '0px',
								'width': '100%',
								'height': $( document ).height() + 'px'
							}
						);
						
						$('#detailedNutritionDhtml').css(
							{
								'top': (nut_label_offset.top + 50) + 'px'
							}
						).show();
						
						$('#detailedNutritionWrapper').show();
						
						$('html,body').animate({scrollTop: $("#detailedNutritionDhtml").offset().top},'slow');
						
						$('#detailedNutritionDhtml .close').click(
							function(){
    							$('#detailedNutritionWrapper').remove();
								return false;
							}
						);
					}
				});
				
			}
			
		</script>
